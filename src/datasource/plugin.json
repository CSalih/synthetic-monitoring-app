{
  "type": "datasource",
  "name": "Worldping Info",
  "id": "worldping-datasource",
  "metrics": true,

  "routes": [
    {
      "path": "dev",
      "method": "GET",
      "url": "{{.JsonData.apiHost}}/api/v1/",
      "reqRole": "Viewer",
      "headers": [
        {
          "name": "Authorization",
          "content": "Bearer {{.SecureJsonData.accessToken}}"
        }
      ]
    },
    {
      "path": "dev",
      "method": "POST",
      "url": "{{.JsonData.apiHost}}/api/v1/",
      "reqRole": "Editor",
      "headers": [
        {
          "name": "Authorization",
          "content": "Bearer {{.SecureJsonData.accessToken}}"
        }
      ]
    },
    {
      "path": "dev",
      "method": "DELETE",
      "url": "{{.JsonData.apiHost}}/api/v1/",
      "reqRole": "Editor",
      "headers": [
        {
          "name": "Authorization",
          "content": "Bearer {{.SecureJsonData.accessToken}}"
        }
      ]
    }
  ],

  "info": {
    "description": "Worldping Info",
    "author": {
      "name": "Grafana"
    },
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    }
  }
}
